{
  "name": "multibaas-for-google-sheets",
  "version": "1.0.0",
  "description": "MultiBaas for Google Sheets",
  "repository": "https://github.com/curvegrid/multibaas-for-google-sheets",
  "author": "Curvegrid Inc.",
  "license": "MIT",
  "private": false,
  "scripts": {
    "lint": "eslint ./*.js src/*.js test/*.js",
    "test": "clasp push && cd test && node mbSheetsAddOn.js",
    "push": "clasp push",
    "update": "sed -i \"s/const VERSION\\ .*/const VERSION\\ =\\ \\'\"$(git describe --tags --match 'v*[0-9].*[0-9].*[0-9]')\"\\';/g\" src/Code.js",
    "update:mac": "sed -i '' \"s/const VERSION\\ .*/const VERSION\\ =\\ \\'\"$(git describe --tags --match 'v*[0-9].*[0-9].*[0-9]')\"\\';/g\" src/Code.js"
  },
  "keywords": [
    "multibaas",
    "ethereum",
    "spreadsheet"
  ],
  "devDependencies": {},
  "dependencies": {
    "eslint": "^7.0.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-plugin-import": "^2.20.2",
    "googleapis": "^51.0.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/**/*.{js,}": [
      "yarn lint"
    ]
  }
}
