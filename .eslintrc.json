    "SpreadsheetApp": false,
    "UrlFetchApp": false,
    // Google spreadsheet test library
    "GasTap": false,
    // Code.js
    "URL_SCHEME": false,
    "URL_BASE": false,
    "MBADDRESS": false,
    "MBBLOCK": false,
    "MBCOMPOSE": false,
    "MBCUSTOMQUERY": false,
